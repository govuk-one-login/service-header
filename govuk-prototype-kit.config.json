{
  "sass": [
    "/dist/styles/service-header-no-imports.scss"
  ],
  "scripts": [
    "/dist/scripts/init-service-header.js"
  ],
  "nunjucksPaths": [
    "/dist/nunjucks"
  ],
  "templates": [
    {
      "name": "Page with GOVUK One Login header",
      "path": "/dist/nunjucks/service-header/templates/service-header.njk",
      "type": "nunjucks"
    }
  ],
  "nunjucksMacros": [
    {
      "macroName": "govukOneLoginServiceHeader",
      "importFrom": "service-header/service-header.njk"
    }
  ],
  "pluginDependencies": [{
    "packageName": "govuk-frontend",
    "minVersion": "5.9.0"
  }]
}